{
    "AWSEBDockerrunVersion" : 2,
    "containerDefenitions": [
        {
            "name":"client",
            "image": "bbalachandran/multi-client",
            "hostname": "client",
            "essential": false,
            "memory": 128        
        },
        {
            "name":"server",
            "image": "bbalachandran/multi-server",
            "hostname": "server",
            "essential": false,
            "memory": 128           
        },
        {
            "name":"worker",
            "image": "bbalachandran/multi-worker",
            "hostname": "worker",
            "essential": false,
            "memory": 128            
        },
        {
            "name":"nginx",
            "image": "bbalachandran/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort":3050
                }
            ],
            "links" :["client","server"],
            "memory": 128             
        }
    ]

}